import math
t=int(input())
if t>=1 and t<=10**5:
    while t>0:
        x=int(input())
        if x>=0 and x<=10**9:
            if x==0 or x==1:
                print(x+1)
            else:
                y=math.floor(math.log(x,2))
                if x==2**(y+1)-1:
                    print(x+1)
                else:
                    print(2**y)
        t=t-1
